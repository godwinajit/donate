.wpfai-list {
	display: table;
	width: 100.2%;
	table-layout: fixed;
	padding: 0;
	margin: 20px 0 30px -2px;
	border: none !important;
}

.wpfai-list li {
	display: table-cell;
	vertical-align: top;
	border-left: 2px solid transparent;
}

.wpfai-list li:before {
	content: "";
}

.wpfai-list a {
	display: block;
	position: relative;
	/*padding-top: 100%;*/
	 padding-bottom: 27%;
    padding-left: 30%;
    padding-top: 35%;    
	color: #fff;
	background: #018752;
	font-size: 36px;
	line-height: 1;
	top: auto;
	left: auto;
	bottom: auto;
	right: auto;
	-webkit-transition: background .3s, color .3s;
	transition: background .3s, color .3s;

}

.wpfai-list a:before {
	position: absolute;
	left: 0;
	right: 0;
	text-align: center;
	top: 50%;
	margin: -18px 0 0;
	font-size: inherit;
	line-height: inherit;
}

.wpfai-list a:after {
	content: "";
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	border: 2px solid #018752;
}

.wpfai-list a:hover {
	color: #018752;
	background: none;
}
.breadcrumb .current-item{
	color: #018752;
