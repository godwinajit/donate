.mc4wp-alert {
  margin: 1em 0;
  padding: 10px 15px;
  color: #c09853;
  background-color: #fcf8e3;
  border: 1px solid #fbeed5;
  border-radius: 2px;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  display: block;
  position: relative;
}

.mc4wp-success {
  color: #468847;
  background-color: #dff0d8;
  border-color: #d6e9c6;
}

.mc4wp-notice {
  color: #3a87ad;
  background-color: #d9edf7;
  border-color: #bce8f1;
}

.mc4wp-error {
  color: #b94a48;
  background-color: #f2dede;
  border-color: #eed3d7;
}


.news-gallery .pagination-holder {
	white-space: nowrap;
	overflow: hidden;
	text-align: center;
}

.news-gallery .pagination {
	display: inline-block!important;
	vertical-align: middle !important;
	overflow: hidden !important;
}

.news-gallery .pagination ul {
	list-style: none;
	padding: 0;
	margin: 0 0 0 -34px;
	white-space: normal;
	font: bold 24px/30px "Libre Baskerville", Georgia, "Times New Roman", Times, serif;
}

.news-gallery .pagination ul li {
	display: inline-block;
	vertical-align: middle;
	padding: 0 0 0 34px;
	position: relative;
}

.news-gallery .pagination ul li:before {
	content: "";
	left: 18px;
	top: 5%;
	width: 1px;
	height: 90%;
	position: absolute;
	background: #000;
	-webkit-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	transform: rotate(45deg);
}

.news-gallery .pagination ul li a {
	color: #333;
	text-decoration: none;
}

.news-gallery .pagination ul li.active a {
	color: #018752;
}
