.divider {
	display: block;
	position: relative;
	overflow: hidden;
	padding: 3px 0;
	background-image: -webkit-linear-gradient(top, #018752 0%, #018752 100%), -webkit-linear-gradient(top, #018752 0%, #018752 100%);
	background-image: linear-gradient(to bottom, #018752 0%, #018752 100%), linear-gradient(to bottom, #018752 0%, #018752 100%);
	background-size: 1px 3px, 1px 3px;
	background-position: 0 50%, 100% 50%;
	background-repeat: no-repeat;
	background-color: transparent !important;
}

.divider:before,
.divider:after {
	content: "";
	position: absolute;
	left: 50%;
	right: 0;
	margin: -1px 0 0 5px;
	height: 1px;
	background: #018752;
	top: 50%;
}

.divider:after {
	right: 50%;
	left: 0;
	margin: -1px 4px 0 0;
}

.divider > * {
	display: block;
	position: relative;
	width: 5px;
	height: 5px;
	border: 1px solid #018752;
	margin: 0 auto;
}

.divider > *:before,
.divider > *:after {
	content: "";
	position: absolute;
	top: 100%;
	left: 50%;
	height: 2px;
	width: 1px;
	background: #018752;
	-webkit-transform: translate(-50%, 2px);
	-ms-transform: translate(-50%, 2px);
	transform: translate(-50%, 2px);
}

.divider > *:after {
	top: auto;
	bottom: 100%;
	-webkit-transform: translate(-50%, -2px);
	-ms-transform: translate(-50%, -2px);
	transform: translate(-50%, -2px);
}

.divider.vertical {
	background-size: 3px 1px, 3px 1px !important;
	background-position: 50% 0, 50% 100% !important;
}

.divider.vertical:before,
.divider.vertical:after {
	left: 50%;
	right: auto;
	margin: 0 0 4px -1px;
	top: 0;
	bottom: 50%;
	height: auto;
	width: 1px;
}

.divider.vertical:after {
	bottom: 0;
	top: 50%;
	margin: 5px 0 0 -1px;
}

.divider.vertical > * {
	position: absolute;
	top: 50%;
	left: 50%;
	margin: -2px 0 0 -3px;
}

.divider.vertical > *:before,
.divider.vertical > *:after {
	width: 2px;
	height: 1px;
	left: 100%;
	top: 50%;
	-webkit-transform: translate(2px, -50%);
	-ms-transform: translate(2px, -50%);
	transform: translate(2px, -50%);
}

.divider.vertical > *:after {
	left: auto;
	right: 100%;
	-webkit-transform: translate(-2px, -50%);
	-ms-transform: translate(-2px, -50%);
	transform: translate(-2px, -50%);
}

.divider.style-dark {
	background-image: -webkit-linear-gradient(top, #333 0%, #333 100%), -webkit-linear-gradient(top, #333 0%, #333 100%);
	background-image: linear-gradient(to bottom, #333 0%, #333 100%), linear-gradient(to bottom, #333 0%, #333 100%);
}

