//init beaker tab
function initBeakerTabs(){

	var activeBtnClass = 'btn-active';
	var activePopupClass = 'popup-active';
	var activeClass = 'active';
	var activeAnimationClass = 'animating';
	var activeAltAnimationClass = 'animating-alt';
	var scaleActive = 'scale-in';
	var animSpeed = 400;
	var win = jQuery(window);
	var header = jQuery("#header");
	var body = jQuery('body');

	jQuery('.animated-tabs').each(function(){
		var animatedTabs = jQuery(this);
		var btnPrev = animatedTabs.find('a.btn-prev');
		var btnNext = animatedTabs.find('a.btn-next');
		var tabset = animatedTabs.find('.tabset');
		var tabHolder = animatedTabs.find('.tab-content');
		var beakerTabs = animatedTabs.find('.beaker-tabs');
		var beakerTabsets = beakerTabs.find('.bubbles-tabset');
		var beakerTabsBtns = beakerTabs.find('a');
		var beakerTabsContent = beakerTabs.find('.beaker-tab-content');
		var beakerBox = tabHolder.find('.beaker-box');
		var slider = animatedTabs.find('.tab-slider');
		var tabsetMask = animatedTabs.find('.tabset-wrap .mask');
		var tabBtnsWrap = tabset.find('li');
		var tabBtns = tabset.find('a');
		var bgBlock = tabHolder.find('.bg');
		var bubblesBlock = tabHolder.find('.bubbles-holder');
		var bubbles = bubblesBlock.find('.bubble');
		var textBlock = tabHolder.find('.add-info');
		var curBtnIndex = tabBtns.filter('.'+activeClass).parent().index();
		var curThemeClass = tabBtns.filter('.'+activeClass).data('rel');
		var animFlag = true;
		var delay = 1100;

		tabHolder.addClass(curThemeClass);

		function sliderHandler(){

			var activeBtn = tabBtnsWrap.eq(curBtnIndex);
			var leftOffset = activeBtn.offset().left - tabset.offset().left + (activeBtn.innerWidth() / 2);
			slider.animate({
				left: leftOffset
			});
			setTimeout(function(){
				animFlag = true;
			}, delay);
		}

		function themeHandler(){
			tabHolder.removeClass(curThemeClass);
			var activeBtn = tabBtnsWrap.eq(curBtnIndex).find('a');
			curThemeClass = activeBtn.data('rel');
			tabHolder.addClass(curThemeClass);
		}


		function maskScrollHandler(){
			var offsetLeft = curBtnIndex * tabBtnsWrap.width();
			tabsetMask.animate({scrollLeft: offsetLeft}, animSpeed);
		}


		btnPrev.on('click', function(e){

			e.preventDefault();


			if(animFlag){
				animFlag = false;
				if(curBtnIndex !== 0){
					curBtnIndex--;
				}
				themeHandler();
				maskScrollHandler();
				sliderHandler();
			}

			if(beakerTabsContent.hasClass(scaleActive)){
				beakerTabsContent.removeClass(scaleActive);
				beakerTabsContent.css({
					height: 0
				});
			}

			beakerTabsBtns.parent().removeClass(activeBtnClass);

		});


		btnNext.on('click', function(e){

			e.preventDefault();


			if(animFlag){
				animFlag = false;
				if(curBtnIndex !== tabBtnsWrap.length - 1){
					curBtnIndex++;
				}
				themeHandler();
				maskScrollHandler();
				sliderHandler();
			}

			if(beakerTabsContent.hasClass(scaleActive)){
				beakerTabsContent.removeClass(scaleActive);
				beakerTabsContent.css({
					height: 0
				});
			}

			beakerTabsBtns.parent().removeClass(activeBtnClass);
		});

		tabsetMask.on('scroll', sliderHandler);

		function bgHeightHandler(){

			//var bgHeight = textBlock.innerHeight() + 130;

			tabHolder.css({
				minHeight: beakerBox.innerHeight() + textBlock.innerHeight()
			});

			/*bgBlock.css({
				bottom: bgHeight
			});

			bubblesBlock.css({
				bottom: bgHeight
			});*/
		}

		sliderHandler();
		bgHeightHandler();

		win.on('load resize orientationchange', function(){
			sliderHandler();
			bgHeightHandler();
		});

			
		tabBtns.each(function(){
			var tabBtn = jQuery(this);
			
			tabBtn.on('click', function(e){
				e.preventDefault();

				if(header.length > 0){
					jQuery('html,body').animate({
						scrollTop: tabset.offset().top - header.innerHeight()
					}, animSpeed * 1.5);
				}else{
					jQuery('html,body').animate({
						scrollTop: tabset.offset().top
					}, animSpeed * 1.5);
				}
				
				body.removeClass(activePopupClass);
				tabBtns.removeClass(activeClass);
				beakerTabsets.removeClass(activeClass);
				tabBtn.addClass(activeClass);
				tabHolder.removeClass(activeClass);
				beakerTabs.removeClass(activeClass);

				if(beakerTabsContent.hasClass(scaleActive)){
					beakerTabsContent.removeClass(scaleActive);
					beakerTabsContent.css({
						height: 0
					});
				}

				beakerTabsBtns.parent().removeClass(activeBtnClass);

				curBtnIndex = tabBtn.parent().index();
				sliderHandler();
				themeHandler();
			});
		});

		beakerTabs.each(function(){
			var hold = jQuery(this);
			var bubblesTabset = hold.find('.bubbles-tabset');
			var btns = hold.find('.bubbles-tabset a');
			var tabContent = hold.find('.beaker-tab-content');
			var tabContentHeight = tabContent.innerHeight();
			var close = hold.find('.tab-close');
			var tabBlocks = hold.find('.bubble-tab');
			var fadeSpeed = 250;

			tabContent.css({
				height: 0
			});

			function animHandler(){
				bubblesBlock.removeClass(activeAnimationClass);
				setTimeout(
					function(){bubblesBlock.addClass(activeAltAnimationClass)}
				, 10);
			}

			function animAltHandler(){
				bubblesBlock.removeClass(activeAltAnimationClass);
				setTimeout(
					function(){bubblesBlock.addClass(activeAnimationClass)}
				, 10);
			}

			btns.each(function(){
				var btn = jQuery(this);
				var href = btn.attr('href');
				var curBlock = jQuery(href);
				
				btn.on('click', function(e){
					e.preventDefault();

					btns.parent().removeClass(activeBtnClass);
					btn.parent().addClass(activeBtnClass);

					if(!bubblesTabset.hasClass(activeClass)){
						bubblesTabset.addClass(activeClass);
					}

					if(!body.hasClass(activePopupClass)){
						body.addClass(activePopupClass);
					}

					animHandler();

					if(!beakerTabs.hasClass(activeClass)){
						beakerTabs.addClass(activeClass);
						tabHolder.addClass(activeClass);
						tabContent.animate({
							height: tabContentHeight
						},{
							duration: animSpeed,
							complete: function(){
								if(!tabContent.hasClass(scaleActive)){
									tabContent.addClass(scaleActive);
								}
							}
						});
					}
					tabBlocks.fadeOut(animSpeed);

					setTimeout(function() {
						curBlock.fadeIn(animSpeed);
					}, animSpeed);
					
					if(header.length > 0){
						jQuery('html,body').animate({
							scrollTop: tabHolder.offset().top - header.innerHeight() + 4
						}, 800);
					}else{
						jQuery('html,body').animate({
							scrollTop: tabHolder.offset().top
						}, 800);
					}
				});
			});
			close.on('click', function(e){
				e.preventDefault();

				animAltHandler();

				btns.parent().removeClass(activeBtnClass);
				beakerTabs.removeClass(activeClass);
				tabHolder.removeClass(activeClass);
				beakerTabsets.removeClass(activeClass);
				body.removeClass(activePopupClass);
				if(tabContent.hasClass(scaleActive)){
					tabContent.removeClass(scaleActive);
				}
				tabBlocks.hide();
				setTimeout(function() {
					tabContent.animate({
						height: 0
					},{
						duration: animSpeed
					});
				}, animSpeed);
			});
			ResponsiveHelper.addRange({
				'..767': {
					on: function() {
						bubblesBlock.addClass(activeAnimationClass);
					},
					off: function(){
						bubblesBlock.removeClass(activeAltAnimationClass);
						bubblesBlock.removeClass(activeAnimationClass);
					}
				}
			});
		});

	});
}


//init check Scroll For Presentation
function initcheckScrollForPresentation() {
	var moving_line = jQuery('.card-html-code .line'),
		front_image = jQuery('.card-html-code .front-image'),
		back_image = jQuery('.card-html-code .back-image'),
		html_code_section = jQuery('#htmlCodeSection');
	
	if (html_code_section.length) {
		html_code_section = html_code_section.offset().top - 200;
		
		jQuery(window).on('scroll', function() {
			var current_scroll = jQuery(this).scrollTop(),
				move_line_distance = (current_scroll - html_code_section) * 1.3;
			
			move_line_distance = Math.ceil(move_line_distance);
			if (current_scroll > html_code_section){
				if(move_line_distance <= 460){
					moving_line.css('left',move_line_distance);
					front_image.css('width', 460 - move_line_distance);
					back_image.css('width', move_line_distance);
				}
			}
		});
	}
}


// init slot machine effect
function initSlotMachineEffect(){
	jQuery('.slot-machine').slotMachine({
		animSpeed: 7000,
		delay: 2000,
		slots: 10
	});
}

// slot machine effect plugin
;(function($){
	function SlotMachine(options){
		this.options = $.extend({
			animSpeed: 8000,
			delay: 2000,
			slots: 15
		},options);
		this.init();
	}
	SlotMachine.prototype = {
		init: function(){
			if (this.options.holder) {
				this.findElements();
				this.generateStructure();
				this.attachEvents();
				this.scrollHandler();
			}
		},
		findElements: function(){
			this.win = $(window);
			this.holder = $(this.options.holder);
			this.animate = false;
		},
		generateStructure: function(){
			var nums = this.holder.text().split('');
			this.holder.empty();
			for (var i = 0; i < nums.length; i++){
				var num = nums[i];
				var randomNumbers = '';
				var structure = '<span class="slot-wrapper"><span class="slot-slider">';
				if (this.isNumber(num)) {
					// insert random number
					for (var j = 0; j < this.options.slots; j++){
						var randomNum = Math.floor(Math.random() * 9);
						structure += '<span class="slot random-number">'+randomNum+'</span>';
						randomNumbers += '<span class="slot random-number">'+randomNum+'</span>';
					}
					// insert main number
					structure += '<span class="slot main-number">'+num+'</span>';
				} else {
					for (var j = 0; j < this.options.slots; j++){
						var randomNum = Math.floor(Math.random() * 9);
						structure += '<span class="slot random-number">'+num+'</span>';
						randomNumbers += '<span class="slot random-number">'+num+'</span>';
					}
					// insert separator
					// structure += '<span class="slot separator">'+num+'</span>';
				}
				structure += randomNumbers + '</span></span>';
				this.holder.append($(structure));
			}
			this.wrapper = this.holder.find('.slot-wrapper');
			this.sliders = this.holder.find('.slot-slider');
		},
		attachEvents: function(){
			var self = this;
			this.win.on('scroll resize orientationchange', function(){
				self.scrollHandler();
			});
			
		},
		scrollHandler: function(){
			if (this.win.scrollTop() < this.holder.offset().top && this.win.scrollTop() + this.win.height() >  this.holder.offset().top){
				this.moveSlots();
			} else {
				this.stopSlots();
			}
		},
		moveSlots: function(){
			var self = this;
			if (!this.animate) {
				this.animate = true;
				this.sliders.each(function(){
					var slider = $(this);
					if (slider.children().length > 1) {
						var offset = self.wrapper.eq(0).height() * self.options.slots;
						slider.animate({marginTop: -offset},{queue: false, easing: 'easeOutElastic', duration: self.options.animSpeed + Math.round(Math.random()*self.options.delay)});
					}
				});
			}
		},
		stopSlots: function(){
			this.animate = false;
			this.sliders.each(function(){
				$(this).stop().removeAttr('style');
			});
		},
		isNumber: function(n){
			return !isNaN(parseFloat(n)) && isFinite(n);
		}
	};
	$.fn.slotMachine = function(opt){
		return this.each(function(){
			$(this).data('SlotMachine', new SlotMachine($.extend(opt, {holder: this})));
		});
	};
}(jQuery));

function fixFlash() {
        jQuery('object').each(function(){
            jQuery(this).find('embed').attr('wmode','transparent');
            if(jQuery(this).find('param[name=wmode]').attr('value') != 'transparent') {
                jQuery(this).prepend('<param name="wmode" value="transparent" />');
            }
            temp_var = jQuery(this).parent().html();
            jQuery(this).parent().html(temp_var);
        });
    }

jQuery(window).load(function(){
	initImageResize();
	initIsotopeFilter();
});

// init masonry
function initMasonry(){
	jQuery('.masonry').each(function() {
		var hold = jQuery(this);
		hold.imagesLoaded( function() {
			hold.masonry({
				itemSelector: '.item',
				columnWidth: 1
			});
		});
	});
}

// init photoslider
function initPhotoSlider(){
	jQuery('.photoslider').photoSlider({
		mask: '.mask',
		slider: '.frame'
	});
}
// init masonry


// content tabs init
function initSubTabs() {
	jQuery('div.dev-holder').fadeGallery({
		slides: 'div.dev-content > div',
		pagerLinks: '.dev-process area',
		activeClass: 'activestate',
		disableWhileAnimating: true,
		autoHeight:true,
		animSpeed:300,
		event: 'mouseenter',
		onInit: function(self){
			self.pagerLinks.each(function(){
				jQuery(this).removeAttr('href');
			});
			self.items = self.gallery.find('.map > ul > li');
			self.slides.css({
				position:'absolute'
			});
		},
		onBeforeChange: function(self){
			if (self.items.length) {
				self.items.removeClass(self.options.activeClass).eq(self.currentIndex).addClass(self.options.activeClass);
			}
		}
	});
}

// init ajax load
function initAjaxLoad() {
	jQuery('.ajax-load').ajaxLoad({
		contentHolder: '.ajax-holder',
		loaderLink: '.hidden-loader',
		btnHolder: '.btn-holder',
		btnLoad: '.link-more',
		contentTag: '.row',
		animSpeed: 400,
		onLoad: function(){
			initLightbox();
		}
	});
}

/*
 * Ajax Load Post plugin
 */
;(function($){
	function AjaxLoad(options){
		this.options = $.extend({
			contentHolder: '.content-holder',
			loaderLink: '.hidden-loader',
			btnHolder: '.btn-holder',
			btnLoad: '.btn-load',
			contentTag: 'li',
			animSpeed: 600
		},options);
		this.init();
	}
	AjaxLoad.prototype = {
		init: function(){
			if (this.options.holder) {
				this.findElements();
				this.attachEvents();
			}
		},
		findElements: function(){
			this.holder = $(this.options.holder);
			this.contentHolder = this.holder.find(this.options.contentHolder);
			this.btnHolder = this.holder.find(this.options.btnHolder);
			this.btnLoad = this.btnHolder.find(this.options.btnLoad);
			this.busy = false;
		},
		attachEvents: function(){
			var self = this;
			var clickHandler = function(e){
				e.preventDefault();
				self.ajaxCall(this.getAttribute('href'));
			};
			this.btnLoad.on('click', clickHandler);
		},
		ajaxCall: function(target){
			var self = this;
			if (!this.busy) {
				this.busy = true;
				$.ajax({
					url: target,
					type: 'get',
					data: 'ajax=1',
					dataType: 'text',
					success: function(msg){
						var data = jQuery(msg)
						var nextLink = data.filter(self.options.loaderLink);
						var content = data.filter(self.options.contentTag);

						self.contentHolder.append(content.css({display:'none'}));
						content.slideDown(self.options.animSpeed);

						if (nextLink.length) {
							self.btnLoad.attr('href', nextLink.attr('href'));
						} else {
							self.btnHolder.slideUp(self.options.animSpeed);
						}	

						self.busy = false;

						// external callback
						self.makeCallback('onLoad', self);
					},
					error: function(){
						busy = false;
					}
				});
			}
		},
		makeCallback: function(name) {
			if(typeof this.options[name] === 'function') {
				var args = Array.prototype.slice.call(arguments);
				args.shift();
				this.options[name].apply(this, args);
			}
		}
	};
	$.fn.ajaxLoad = function(opt){
		return this.each(function(){
			$(this).data('AjaxLoad', new AjaxLoad($.extend(opt, {holder: this})));
		});
	};
}(jQuery));

/*
 * jQuery Photo Slider Plugin
 */
;(function($){
	function PhotoSlider(options) {
		this.options = $.extend({
			slider: '.slider'
		}, options);
		this.init();
	}
	PhotoSlider.prototype = {
		init: function() {
			if(this.options.holder) {
				this.findElements();
				this.attachEvents();
				this.makeCallback('onInit', this);
			}
		},
		findElements: function() {
			var self = this;
			this.body = $('body');
			this.holder = $(this.options.holder);
			this.mask = this.holder.find(this.options.mask);
			this.slider = this.holder.find(this.options.slider);
			this.hover = false;
			this.mask.css({msTouchAction: 'none'});
		},
		attachEvents: function() {
			// add handler
			var self = this;
			this.moveHandler = function(e){
				e = e.originalEvent.changedTouches ? e.originalEvent.changedTouches[0] : e;
				if (self.hover) {
					var width = Math.ceil(e.pageX - self.holder.offset().left)*100/self.mask.width() + '%';
					if (e.pageX < self.mask.offset().left){
						width = 0;
					} else if (e.pageX > self.mask.offset().left + self.mask.width()) {
						width = '100%';
					}
					self.slider.css({
						width: width
					});
				}
				e.preventDefault();
			};
			this.mask.on('mouseenter touchstart pointerdown MSPointerDown selectstart', function(e){
				self.hover = true;
				self.moveHandler(e);
				e.preventDefault();
			}).on('mouseleave touchend pointerup MSPointerUp', function(e){
				setTimeout(function(){
					self.hover = false;
				},0);
				e.preventDefault();
			});
			this.body.on('mousemove touchmove pointermove MSPointerMove', this.moveHandler);
		},
		makeCallback: function(name) {
			if(typeof this.options[name] === 'function') {
				var args = Array.prototype.slice.call(arguments);
				args.shift();
				this.options[name].apply(this, args);
			}
		}
	};

	// jQuery plugin interface
	$.fn.photoSlider = function(opt) {
		return this.each(function() {
			jQuery(this).data('PhotoSlider', new PhotoSlider($.extend(opt, {holder: this})));
		});
	};
}(jQuery));

// animation start
function initAnimationSolutions() {
	if(!jQuery('#solutions').length ) {return;}
	var holder = jQuery('#solutions');
	var holderClass = 'animate';
	var holderPos = holder.offset().top;
	var page = jQuery(window);
	var flag = false;
	function addClassToHolder () {
		var scrollPos = page.scrollTop();
		if(scrollPos >= holderPos){
			holder.addClass(holderClass);
			if(!flag){
				flag = true;
				initTabs();
			}
		} else{
			holder.removeClass(holderClass);
			if(flag){
				flag = false;
				destroyTabs();
			}
		}
	}
	addClassToHolder();
	$(document).on('scroll', function () {addClassToHolder();});
}

// content tabs destroy
function destroyTabs(){jQuery('ul.solutions-icons').trigger('destroy');}

// content tabs init
function initTabs() {
	jQuery('ul.solutions-icons').contentTabs({
		addToParent: true,
		autoRotate: true,
		switchTime: 3000,
		animSpeed: 400,
		effect: 'none',
		tabLinks: 'a'
	});
	jQuery('ul.tabset').contentTabs({
		animSpeed: 300,
		effect: 'none',
		tabLinks: 'a'
	});
}

/*
 * jQuery Tabs plugin
 */
;(function($){
	$.fn.contentTabs = function(o){
		// default options
		var options = $.extend({
			activeClass:'active',
			addToParent:false,
			autoHeight:false,
			autoRotate:false,
			checkHash:false,
			animSpeed:400,
			switchTime:3000,
			effect: 'none', // "fade", "slide"
			tabLinks:'a',
			attrib:'href',
			event:'click'
		},o);

		return this.each(function(){
			var tabset = $(this), tabs = $();
			var tabLinks = tabset.find(options.tabLinks);
			var tabLinksParents = tabLinks.parent();
			var prevActiveLink = tabLinks.eq(0), currentTab, animating;
			var tabHolder;

			var destoryHandler = function(){
				clearTimeout(rotationTimer);
				tabset.off('destroy', destoryHandler);
				tabLinks.unbind(options.event);
				tabs.removeAttr('style').removeClass(tabHiddenClass);
			}

			tabset.on('destroy', destoryHandler);

			// handle location hash
			if(options.checkHash && tabLinks.filter('[' + options.attrib + '="' + location.hash + '"]').length) {
				(options.addToParent ? tabLinksParents : tabLinks).removeClass(options.activeClass);
				setTimeout(function() {
					window.scrollTo(0,0);
				},1);
			}

			// init tabLinks
			tabLinks.each(function(){
				var link = $(this);
				var href = link.attr(options.attrib);
				var parent = link.parent();
				href = href.substr(href.lastIndexOf('#'));

				// get elements
				var tab = $(href).hide();
				tabs = tabs.add(tab);
				link.data('cparent', parent);
				link.data('ctab', tab);

				// find tab holder
				if(!tabHolder && tab.length) {
					tabHolder = tab.parent();
				}

				// show only active tab
				var classOwner = options.addToParent ? parent : link;
				if(classOwner.hasClass(options.activeClass) || (options.checkHash && location.hash === href)) {
					classOwner.addClass(options.activeClass);
					prevActiveLink = link; currentTab = tab;
					tab.removeClass(tabHiddenClass).width('');
					contentTabsEffect[options.effect].show({tab:tab, fast:true});
				} else {
					var tabWidth = tab.width();
					if(tabWidth) {
						tab.width(tabWidth);
					}
					tab.addClass(tabHiddenClass);
				}

				// event handler
				link.bind(options.event, function(e){
					if(link != prevActiveLink && !animating) {
						switchTab(prevActiveLink, link);
						prevActiveLink = link;
						clearTimeout(rotationTimer);
						options.autoRotate = false;
					}
				});
				if(options.attrib === 'href') {
					link.bind('click', function(e){
						e.preventDefault();
					});
				}
			});

			// tab switch function
			function switchTab(oldLink, newLink) {
				animating = true;
				var oldTab = oldLink.data('ctab');
				var newTab = newLink.data('ctab');
				prevActiveLink = newLink;
				currentTab = newTab;

				// refresh pagination links
				(options.addToParent ? tabLinksParents : tabLinks).removeClass(options.activeClass);
				(options.addToParent ? newLink.data('cparent') : newLink).addClass(options.activeClass);

				// hide old tab
				resizeHolder(oldTab, true);
				contentTabsEffect[options.effect].hide({
					speed: options.animSpeed,
					tab:oldTab,
					complete: function() {
						// show current tab
						resizeHolder(newTab.removeClass(tabHiddenClass).width(''));
						contentTabsEffect[options.effect].show({
							speed: options.animSpeed,
							tab:newTab,
							complete: function() {
								if(!oldTab.is(newTab)) {
									oldTab.width(oldTab.width()).addClass(tabHiddenClass);
								}
								animating = false;
								resizeHolder(newTab, false);
								autoRotate();
							}
						});
					}
				});
			}

			// holder auto height
			function resizeHolder(block, state) {
				var curBlock = block && block.length ? block : currentTab;
				if(options.autoHeight && curBlock) {
					tabHolder.stop();
					if(state === false) {
						tabHolder.css({height:''});
					} else {
						var origStyles = curBlock.attr('style');
						curBlock.show().css({width:curBlock.width()});
						var tabHeight = curBlock.outerHeight(true);
						if(!origStyles) curBlock.removeAttr('style'); else curBlock.attr('style', origStyles);
						if(state === true) {
							tabHolder.css({height: tabHeight});
						} else {
							tabHolder.animate({height: tabHeight}, {duration: options.animSpeed});
						}
					}
				}
			}
			if(options.autoHeight) {
				$(window).bind('resize orientationchange', function(){
					tabs.not(currentTab).removeClass(tabHiddenClass).show().each(function(){
						var tab = jQuery(this), tabWidth = tab.css({width:''}).width();
						if(tabWidth) {
							tab.width(tabWidth);
						}
					}).hide().addClass(tabHiddenClass);

					resizeHolder(currentTab, false);
				});
			}

			// autorotation handling
			var rotationTimer;
			function nextTab() {
				var activeItem = (options.addToParent ? tabLinksParents : tabLinks).filter('.' + options.activeClass);
				var activeIndex = (options.addToParent ? tabLinksParents : tabLinks).index(activeItem);
				var newLink = tabLinks.eq(activeIndex < tabLinks.length - 1 ? activeIndex + 1 : 0);
				prevActiveLink = tabLinks.eq(activeIndex);
				switchTab(prevActiveLink, newLink);
			}
			function autoRotate() {
				if(options.autoRotate && tabLinks.length > 1) {
					clearTimeout(rotationTimer);
					rotationTimer = setTimeout(function() {
						if(!animating) {
							nextTab();
						} else {
							autoRotate();
						}
					}, options.switchTime);
				}
			}
			autoRotate();
		});
	};

	// add stylesheet for tabs on DOMReady
	var tabHiddenClass = 'js-tab-hidden';
	$(function() {
		var tabStyleSheet = $('<style type="text/css">')[0];
		var tabStyleRule = '.'+tabHiddenClass;
		tabStyleRule += '{position:fixed !important;left:-9999px !important;top:-9999px !important;display:block !important}';
		if (tabStyleSheet.styleSheet) {
			tabStyleSheet.styleSheet.cssText = tabStyleRule;
		} else {
			tabStyleSheet.appendChild(document.createTextNode(tabStyleRule));
		}
		$('head').append(tabStyleSheet);
	});

	// tab switch effects
	var contentTabsEffect = {
		none: {
			show: function(o) {
				o.tab.css({display:'block'});
				if(o.complete) o.complete();
			},
			hide: function(o) {
				o.tab.css({display:'none'});
				if(o.complete) o.complete();
			}
		},
		fade: {
			show: function(o) {
				if(o.fast) o.speed = 1;
				o.tab.fadeIn(o.speed);
				if(o.complete) setTimeout(o.complete, o.speed);
			},
			hide: function(o) {
				if(o.fast) o.speed = 1;
				o.tab.fadeOut(o.speed);
				if(o.complete) setTimeout(o.complete, o.speed);
			}
		},
		slide: {
			show: function(o) {
				var tabHeight = o.tab.show().css({width:o.tab.width()}).outerHeight(true);
				var tmpWrap = $('<div class="effect-div">').insertBefore(o.tab).append(o.tab);
				tmpWrap.css({width:'100%', overflow:'hidden', position:'relative'}); o.tab.css({marginTop:-tabHeight,display:'block'});
				if(o.fast) o.speed = 1;
				o.tab.animate({marginTop: 0}, {duration: o.speed, complete: function(){
					o.tab.css({marginTop: '', width: ''}).insertBefore(tmpWrap);
					tmpWrap.remove();
					if(o.complete) o.complete();
				}});
			},
			hide: function(o) {
				var tabHeight = o.tab.show().css({width:o.tab.width()}).outerHeight(true);
				var tmpWrap = $('<div class="effect-div">').insertBefore(o.tab).append(o.tab);
				tmpWrap.css({width:'100%', overflow:'hidden', position:'relative'});

				if(o.fast) o.speed = 1;
				o.tab.animate({marginTop: -tabHeight}, {duration: o.speed, complete: function(){
					o.tab.css({display:'none', marginTop:'', width:''}).insertBefore(tmpWrap);
					tmpWrap.remove();
					if(o.complete) o.complete();
				}});
			}
		}
	};
}(jQuery));


/*
 * Responsive Layout helper
 */
ResponsiveHelper = (function($){
	// init variables
	var handlers = [],
		prevWinWidth,
		win = $(window),
		nativeMatchMedia = false;

	// detect match media support
	if(window.matchMedia) {
		if(window.Window && window.matchMedia === Window.prototype.matchMedia) {
			nativeMatchMedia = true;
		} else if(window.matchMedia.toString().indexOf('native') > -1) {
			nativeMatchMedia = true;
		}
	}

	// prepare resize handler
	function resizeHandler() {
		var winWidth = win.width();
		if(winWidth !== prevWinWidth) {
			prevWinWidth = winWidth;

			// loop through range groups
			$.each(handlers, function(index, rangeObject){
				// disable current active area if needed
				$.each(rangeObject.data, function(property, item) {
					if(item.currentActive && !matchRange(item.range[0], item.range[1])) {
						item.currentActive = false;
						if(typeof item.disableCallback === 'function') {
							item.disableCallback();
						}
					}
				});

				// enable areas that match current width
				$.each(rangeObject.data, function(property, item) {
					if(!item.currentActive && matchRange(item.range[0], item.range[1])) {
						// make callback
						item.currentActive = true;
						if(typeof item.enableCallback === 'function') {
							item.enableCallback();
						}
					}
				});
			});
		}
	}
	win.bind('load resize orientationchange', resizeHandler);

	// test range
	function matchRange(r1, r2) {
		var mediaQueryString = '';
		if(r1 > 0) {
			mediaQueryString += '(min-width: ' + r1 + 'px)';
		}
		if(r2 < Infinity) {
			mediaQueryString += (mediaQueryString ? ' and ' : '') + '(max-width: ' + r2 + 'px)';	
		}
		return matchQuery(mediaQueryString, r1, r2);
	}

	// media query function
	function matchQuery(query, r1, r2) {
		if(window.matchMedia && nativeMatchMedia) {
			return matchMedia(query).matches;
		} else if(window.styleMedia) {
			return styleMedia.matchMedium(query);
		} else if(window.media) {
			return media.matchMedium(query);
		} else {
			return prevWinWidth >= r1 && prevWinWidth <= r2;
		}
	}

	// range parser
	function parseRange(rangeStr) {
		var rangeData = rangeStr.split('..');
		var x1 = parseInt(rangeData[0], 10) || -Infinity;
		var x2 = parseInt(rangeData[1], 10) || Infinity;
		return [x1, x2].sort(function(a, b){
			return a - b;
		});
	}

	// export public functions
	return {
		addRange: function(ranges) {
			// parse data and add items to collection
			var result = {data:{}};
			$.each(ranges, function(property, data){
				result.data[property] = {
					range: parseRange(property),
					enableCallback: data.on,
					disableCallback: data.off
				};
			});
			handlers.push(result);

			// call resizeHandler to recalculate all events
			prevWinWidth = null;
			resizeHandler();
		}
	};
}(jQuery));



// initialize fixed blocks on scroll
function initAnchors() {
	new SmoothScroll({
		anchorLinks: '.anchors .sub-menu a[href^="#"]',
		extraOffset:142
	});
	ResponsiveHelper.addRange({
			'..767': {
				on: function() {
					new SmoothScroll({
						anchorLinks: '.anchors .sub-menu a[href^="#"]',
						extraOffset:100
					});
				}
			}
		});
}

/*!
 * SmoothScroll module
 */
;(function($, exports) {

	// private variables
	var page,
		win = $(window),
		activeBlock, activeWheelHandler,
		wheelEvents = ('onwheel' in document || document.documentMode >= 9 ? 'wheel' : 'mousewheel DOMMouseScroll');

	// animation handlers
	function scrollTo(offset, options, callback) {
		// initialize variables
		var scrollBlock;
		if(document.body) {
			if(typeof options === 'number') {
				options = { duration: options };
			} else {
				options = options || {};
			}
			page = page || $('html, body');
			scrollBlock = options.container || page;
		} else {
			return;
		}

		// treat single number as scrollTop
		if(typeof offset === 'number') {
			offset = { top: offset };
		}

		// handle mousewheel/trackpad while animation is active
		if(activeBlock && activeWheelHandler) {
			activeBlock.off('mousewheel', activeWheelHandler);
		}
		if(options.wheelBehavior && options.wheelBehavior !== 'none') {
			activeWheelHandler = function(e) {
				if(options.wheelBehavior === 'stop') {
					scrollBlock.off('mousewheel', activeWheelHandler);
					scrollBlock.stop();
				} else if(options.wheelBehavior === 'ignore') {
					e.preventDefault();
				}
			};
			activeBlock = scrollBlock.on('mousewheel', activeWheelHandler);
		}

		// start scrolling animation
		scrollBlock.stop().animate({
			scrollLeft: offset.left,
			scrollTop: offset.top
		}, options.duration, function(){
			if(activeWheelHandler) {
				scrollBlock.off('mousewheel', activeWheelHandler);
			}
			if($.isFunction(callback)) {
				callback();
			}
		});
	}

	// smooth scroll contstructor
	function SmoothScroll(options) {
		this.options = $.extend({
			anchorLinks: 'a[href^="#"]',	// selector or jQuery object
			container: null,		// specify container for scrolling (default - whole page)
			extraOffset: null,		// function or fixed number
			activeClasses: null,	// null, "link", "parent"
			easing: 'swing',		// easing of scrolling
			animMode: 'duration',	// or "speed" mode
			animDuration: 800,		// total duration for scroll (any distance)
			animSpeed: 1500,		// pixels per second
			anchorActiveClass: 'anchor-active',
			sectionActiveClass: 'section-active',
			wheelBehavior: 'stop', // "stop", "ignore" or "none"
			useNativeAnchorScrolling: false // do not handle click in devices with native smooth scrolling
		}, options);
		this.init();
	}
	SmoothScroll.prototype = {
		init: function() {
			this.initStructure();
			this.attachEvents();
		},
		initStructure: function(options) {
			this.container = this.options.container ? $(this.options.container) : $('html,body');
			this.scrollContainer = this.options.container ? this.container : win;
			this.anchorLinks = $(this.options.anchorLinks);
		},
		getAnchorTarget: function(link) {
			// get target block from link href
			var targetId = $(link).attr('href');
			return $(targetId.length > 1 ? targetId : 'html');
		},
		getTargetOffset: function(block) {
			// get target offset
			var blockOffset = block.offset().top;
			if(this.options.container) {
				blockOffset -= this.container.offset().top - this.container.prop('scrollTop');
			}

			// handle extra offset
			if(typeof this.options.extraOffset === 'number') {
				blockOffset -= this.options.extraOffset;
			} else if(typeof this.options.extraOffset === 'function') {
				blockOffset -= this.options.extraOffset(block);
			}
			return {top: blockOffset};
		},
		attachEvents: function() {
			var self = this;

			// handle active classes
			if(this.options.activeClasses) {
				// cache structure
				this.anchorData = [];
				this.anchorLinks.each(function() {
					var link = jQuery(this),
						targetBlock = self.getAnchorTarget(link),
						anchorDataItem;

					$.each(self.anchorData, function(index, item) {
						if(item.block[0] === targetBlock[0]) {
							anchorDataItem = item;
						}
					});

					if(anchorDataItem) {
						anchorDataItem.link = anchorDataItem.link.add(link);
					} else {
						self.anchorData.push({
							link: link,
							block: targetBlock
						});
					}
				});

				// add additional event handlers
				this.resizeHandler = function() {
					self.recalculateOffsets();
				};
				this.scrollHandler = function() {
					self.refreshActiveClass();
				};

				this.recalculateOffsets();
				this.scrollContainer.on('scroll', this.scrollHandler);
				win.on('resize', this.resizeHandler);
			}

			// handle click event
			this.clickHandler = function(e) {
				self.onClick(e);
			};
			if(!this.options.useNativeAnchorScrolling) {
				this.anchorLinks.on('click', this.clickHandler);
			}
		},
		recalculateOffsets: function() {
			var self = this;
			$.each(this.anchorData, function(index, data) {
				data.offset = self.getTargetOffset(data.block);
				data.height = data.block.outerHeight();
			});
			this.refreshActiveClass();
		},
		refreshActiveClass: function() {
			var self = this,
				foundFlag = false,
				winHeight = win.height(),
				containerHeight = this.container.prop('scrollHeight'),
				viewPortHeight = this.scrollContainer.height(),
				scrollTop = this.options.container ? this.container.prop('scrollTop') : win.scrollTop();

			// user function instead of default handler
			if(this.options.customScrollHandler) {
				this.options.customScrollHandler.call(this, scrollTop, this.anchorData);
				return;
			}

			// sort anchor data by offsets
			this.anchorData.sort(function(a, b) {
				return a.offset.top - b.offset.top;
			});
			function toggleActiveClass(anchor, block, state) {
				anchor.toggleClass(self.options.anchorActiveClass, state);
				block.toggleClass(self.options.sectionActiveClass, state);
			}

			// default active class handler
			$.each(this.anchorData, function(index) {
				var reverseIndex = self.anchorData.length - index - 1,
					data = self.anchorData[reverseIndex],
					anchorElement = (self.options.activeClasses === 'parent' ? data.link.parent() : data.link);

				if(scrollTop >= containerHeight - viewPortHeight) {
					// handle last section
					if(reverseIndex === self.anchorData.length - 1) {
						toggleActiveClass(anchorElement, data.block, true);
					} else {
						toggleActiveClass(anchorElement, data.block, false);
					}
				} else {
					// handle other sections
					if(!foundFlag && (scrollTop >= data.offset.top - 1 || reverseIndex === 0) ) {
						foundFlag = true;
						toggleActiveClass(anchorElement, data.block, true);
					} else {
						toggleActiveClass(anchorElement, data.block, false);
					}
				}
			});
		},
		calculateScrollDuration: function(offset) {
			var distance;
			if(this.options.animMode === 'speed') {
				distance = Math.abs(this.scrollContainer.scrollTop() - offset.top);
				return (distance / this.options.animSpeed) * 1000;
			} else {
				return this.options.animDuration;
			}
		},
		onClick: function(e) {
			var targetBlock = this.getAnchorTarget(e.currentTarget),
				targetOffset = this.getTargetOffset(targetBlock);

			e.preventDefault();
			scrollTo(targetOffset, {
				container: this.container,
				wheelBehavior: this.options.wheelBehavior,
				duration: this.calculateScrollDuration(targetOffset),
			});
		},
		destroy: function() {
			if(this.options.activeClasses) {
				win.off('resize', this.resizeHandler);
				this.scrollContainer.off('scroll', this.scrollHandler);
			}
			this.anchorLinks.off('click', this.clickHandler);
		}
	};

	// public API
	$.extend(SmoothScroll, {
		scrollTo: function(blockOrOffset, durationOrOptions, callback) {
			scrollTo(blockOrOffset, durationOrOptions, callback);
		}
	});

	// export module
	exports.SmoothScroll = SmoothScroll;
}(jQuery, this));

function initIsotopeFilter(){
	jQuery('.isotope-filter').each(function(){
		var holder = jQuery(this),
			container = holder.find('.isotop-items-holder'),
			filterForm = holder.find('.filter-form'),
			selects = filterForm.find('select'), 
			input = filterForm.find('input[type="search"]'),
			timer;
		// initialize isotope
		container.isotope({
			itemSelector : '.isotop-item'
		});
		
		function getFilterSelector(){
			var selector = '';
			
			selects.each(function(){
				var select = jQuery(this);
				
				if(this.selectedIndex){
					selector += select.val();
				}
			});
			
			if(input.val().length){
				selector += ':containsInsensitive(' + input.val() + ')';
			}
			
			if (!selector) {
				selector = "*";
			}
			
			return selector;
		}
		
		function filter(searchParams){
			container.isotope({
				filter:searchParams
			});
		}
		
		selects.on('change', function (){
			filter(getFilterSelector());
		});
		
		input.on('keydown keyup keypress', function(){
			clearTimeout(timer);
			setTimeout(function(){
				filter(getFilterSelector());
			}, 500);
		});
	});
}

jQuery.expr[":"].containsInsensitive = jQuery.expr.createPseudo(function(arg) {
	return function( elem ) {
		return jQuery(elem).text().toUpperCase().indexOf(arg.toUpperCase()) >= 0
	};
});


// initialize custom form elements
function initCustomForms() {
	jcf.setOptions('Select', {
		wrapNative: false,
		fakeDropInBody: false
	});
	jcf.replaceAll();
}

// fancybox modal popup init
function initLightbox(){
	var k = 0.8;
	var ratio = 0.5625;
	var maxWidth = 1700;
	var win = jQuery(window);
	var width = win.width() - 40 < maxWidth ? win.width() - 40 : maxWidth;

	;(function() {
		var styleSheet;

		var getWidth = function() {
			return win.width() - 40 < maxWidth ? win.width() - 40 : maxWidth;
		};

		var createStyleTag = function() {
			// create style tag
			var styleTag = jQuery('<style>').appendTo('head');
			styleSheet = styleTag.prop('sheet') || styleTag.prop('styleSheet');

			// crossbrowser style handling
			var addCSSRule = function(selector, rules, index) {
				if(styleSheet.insertRule) {
					styleSheet.insertRule(selector + '{' + rules + '}', index);
				} else {
					styleSheet.addRule(selector, rules, index);
				}
			};

			// create style rules
			addCSSRule('.lightbox-work', 'max-width: none');
			resizeHandler();
		};

		var resizeHandler = function() {
			// handle changes in style rules
			var styleRules = styleSheet.cssRules || styleSheet.rules;
			styleRules[0].style.maxWidth = getWidth() + 'px';
		};

		createStyleTag();
		win.on('resize orientationchange', resizeHandler);
	}());
	
	jQuery('a.lightbox, a[data-rel*="lightbox"]').each(function() {
		var link = jQuery(this);
		var iframe = link.hasClass('iframe');
		link.attr('rel', link.attr('data-rel')).fancybox({
			padding: 0,
			margin: 20,
			width: width,
			height: width*ratio,
			cyclic: true,
			autoScale: true,
			overlayShow: true,
			overlayOpacity: 0.65,
			overlayColor: '#000000',
			titlePosition: 'inside',
			
			onStart: function() {
				if(iframe){
					win.bind('resize orientationchange', resizeIframe);
				}
				win.bind('resize orientationchange', resizeLightbox);
			},
			
			onComplete: function(box) {
				if(link.attr('href').indexOf('#') === 0) {
					jQuery('#fancybox-content').find('a.close').unbind('click.fb').bind('click.fb', function(e){
						jQuery.fancybox.close();
						e.preventDefault();
					});
				}
				resizeIframe();
				resizeLightbox();
			},
			
			onClosed: function() {
				if (iframe) {
					win.unbind('resize orientationchange', resizeIframe);
				}
				win.unbind('resize orientationchange', resizeLightbox);
			}
		});
		function resizeIframe(){
			width = win.width() - 40 < maxWidth ? win.width() - 40 : maxWidth;
			jQuery('#fancybox-frame').css({
				width: width,
				height: width*ratio
			});
		}
	});
	function resizeLightbox(){
		jQuery('#fancybox-content, #fancybox-wrap').css({
			width:'auto',
			height: 'auto'
		});
		jQuery.fancybox.resize();
		jQuery.fancybox.center();
	}
}


/* Fancybox overlay fix */
jQuery(function(){
	// detect device type
	var isTouchDevice = ('ontouchstart' in window) || window.DocumentTouch && document instanceof DocumentTouch;
	var isWinPhoneDevice = /Windows Phone/.test(navigator.userAgent);

	// create stylesheet
	var styleTag = jQuery('<style>').appendTo('head');
	styleSheet = styleTag.prop('sheet') || styleTag.prop('styleSheet');

	// crossbrowser style handling
	var addCSSRule = function(selector, rules, index) {
		if(styleSheet.insertRule) {
			styleSheet.insertRule(selector + '{' + rules + '}', index);
		} else {
			styleSheet.addRule(selector, rules, index);
		}
	};

	if(isTouchDevice || isWinPhoneDevice) {
		// custom positioning for mobile devices
		setTimeout(function() {
			var page = jQuery('body'),
				fancySideShadows = jQuery('#fancybox-bg-ne, #fancybox-bg-e, #fancybox-bg-se'),
				fancyOverlay = jQuery('#fancybox-overlay'),
				fancyWrap = jQuery('#fancybox-wrap');

			// override position handler
			jQuery.fancybox.center = function() {
				if(fancyWrap.innerWidth() > fancyOverlay.innerWidth()) {
					fancyWrap.css({padding: 0});
					fancySideShadows.css({display: 'none'});
				} else {
					fancyWrap.css({padding: ''});
					fancySideShadows.css({display: ''});
				}
				fancyWrap.css({
					left: (page.innerWidth() - fancyWrap.innerWidth()) / 2
				});
			};
			jQuery(window).on('resize orientationchange', function() {
				jQuery.fancybox.center();
			});
		}, 1);
	} else {
		// use position fixed for desktop
		addCSSRule('#fancybox-overlay', 'position:fixed;top:0;left:0');
	}
});

// align blocks height
function initSameHeight(){
	jQuery('.same-height-holder').sameHeight({
		elements: '.same-height',
		flexible: true,
		multiLine: true
	});
	jQuery('.same-height-holder').sameHeight({
		elements: '.heading',
		flexible: true,
		multiLine: true
	});
	jQuery('.same-height-holder').sameHeight({
		elements: '.item',
		flexible: true,
		multiLine: true
	});
	jQuery('.partners-list').sameHeight({
		elements: '.block',
		flexible: true,
		multiLine: true
	});
	jQuery('.typography-block .row').sameHeight({
		elements: 'div',
		flexible: true,
		multiLine: true
	});
	jQuery('.tabset').sameHeight({
		elements: 'li',
		flexible: true,
		multiLine: true
	});
}

