.photoslider {
  width: 90%;
  margin: 0 auto;
  border: 1px solid #d5d4d4;
  -webkit-border-radius: 8px;
  border-radius: 8px;
  background: #f5f5f5;
  padding: 45px 0 13px;
  position: relative;
}
.photoslider:before, .photoslider:after {
  content: '';
  position: absolute;
  top: 19px;
  left: 25px;
  background: #d2d5d8;
  width: 10px;
  height: 10px;
  -webkit-border-radius: 50%;
  border-radius: 50%;
}
.photoslider:before {
  left: 51px;
}
.photoslider .mask {
  cursor: col-resize;
  position: relative;
  border-style: solid;
  border-width: 1px 0;
  border-color: #d5d4d4 transparent;
  }
.photoslider img {
  margin: 0;
  width: 100%;
}
.photoslider .frame {
  position: absolute;
  top: -10.7%;
  left: 0;
  width: 50%;
  bottom: -8%;
}
.photoslider .frame .hold {
  overflow: hidden;
  position: absolute;
  right: 0;
  top: 9%;
  bottom: 6.7%;
  left: 0;
}
.photoslider .frame .hold img {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  max-width: none;
  height: 100%;
    width: auto;
  z-index: 1;
  }
.photoslider .frame .divider {
  position: absolute;
  right: 0;
  top: 0;
  bottom: 0;
  width: 3px;
  height: auto;
  z-index: 5;
  background: #252525;
}
.photoslider .frame .divider:before, .photoslider .frame .divider:after {
  content: '';
  position: absolute;
  top: 0;
  background: white;
  right: -10px;
  width: 22px;
  height: 22px;
  border: 1px solid #252525;
  -webkit-border-radius: 50%;
  border-radius: 50%;
  z-index: 10;
}
.photoslider .frame .divider:before {
  top: auto;
  bottom: 0;
}

