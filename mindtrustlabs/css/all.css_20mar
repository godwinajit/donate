@-webkit-viewport {
  width: device-width;
}
@-moz-viewport {
  width: device-width;
}
@-ms-viewport {
  width: device-width;
}
@-o-viewport {
  width: device-width;
}
@viewport {
  width: device-width;
}
* {
  max-height: 20000px;
  -moz-osx-font-smoothing: grayscale;
  -webkit-font-smoothing: antialiased;
  -webkit-text-size-adjust: 100%;
  -ms-text-size-adjust: none;
}

body {
  min-width: 320px;
}
@media only screen and (max-width: 767px) {
  body {
    -webkit-transition: all 0.3s linear;
    -o-transition: all 0.3s linear;
    transition: all 0.3s linear;
  }
  body.menu-active {
    margin: 0 240px 0 -240px;
  }
  body.menu-active .top-section {
    right: 240px;
    left: -240px;
    -webkit-transition: all 0.3s linear;
    -o-transition: all 0.3s linear;
    transition: all 0.3s linear;
  }
}

input[type=text], input[type=file], textarea {
  -webkit-appearance: none;
  -webkit-border-radius: 0;
}

input[type=submit] {
  -webkit-border-radius: 0;
  -webkit-appearance: none;
}

img {
  max-width: 100%;
  height: auto;
}

a,
a:hover {
  color: #3b93af;
}

video, iframe, object, embed {
  max-width: 100%;
}

input::-moz-focus-inner {
  border: 0;
  padding: 0;
}

button,
input[type=submit],
input[type=button] {
  cursor: pointer;
}

button, input {
  width: auto;
  overflow: visible;
}

figure {
  position: relative;
}

button::-moz-focus-inner {
  padding: 0;
  border: 0;
}

blockquote, blockquote:before, blockquote:after, q, q:before, q:after {
  quotes: none;
}

blockquote:before, blockquote:after, q:before, q:after {
  content: '';
}

div.aligncenter {
  margin: 0 auto 10px;
}

img.aligncenter {
  display: block;
  margin: 0 auto;
}

p {
  font-weight: 300;
}

.masonry {
  position: relative;
  max-width: 1170px;
}
.masonry * {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.masonry .item {
  width: 41.6667%;
  float: left;
  padding: 0 15px 30px;
}
@media only screen and (max-width: 500px) {
  .masonry .item {
    padding: 0 10px 10px;
  }
}
.masonry .item.w2 {
  width: 58.3333%;
}
.masonry .item.w2 object, .masonry .item.w2 embed {
  height: 360px;
}
.masonry .item.w3 {
  width: 67.5%;
}
.masonry .item.w4 {
  width: 32.5%;
}
.masonry .item.w5 {
  width: 50%;
}
.masonry .item.full {
  width: 100%;
}
@media only screen and (max-width: 1199px) {
  .masonry object, .masonry embed {
    height: 280px !important;
  }
}
@media only screen and (max-width: 991px) {
  .masonry object, .masonry embed {
    height: 200px !important;
  }
}
@media only screen and (max-width: 800px) {
  .masonry iframe {
    height: 236px;
  }
  .masonry .fullwidth-video {
    height: 500px;
  }
}
@media only screen and (max-width: 760px) {
  .masonry object, .masonry embed {
    height: 175px !important;
  }
}
@media only screen and (max-width: 500px) {
  .masonry iframe {
    height: 140px;
  }
  .masonry object, .masonry embed {
    height: 140px !important;
  }
}
@media only screen and (max-width: 479px) {
  .masonry iframe {
    height: 100px;
  }
  .masonry object, .masonry embed {
    height: 100px !important;
  }
}

#wrapper .masonry img {
  margin: 0;
  max-width: none;
  width: 100%;
  height: auto;
}

.social-networks, .social {
  margin: 0;
  padding: 0;
  list-style: none;
}

.alignleft {
  float: left;
  margin: 0 10px 10px 0;
}

.alignright {
  float: right;
  margin: 0 0 10px 10px;
}

#fullvideo {
  min-height: 600px;
}
@media only screen and (max-width: 800px) {
  #fullvideo {
    min-height: 410px;
  }
}
#fullvideo .video-frame:after {
  display: none;
}

#wrapper {
  width: 100%;
  overflow: hidden;
  position: relative;
}
#wrapper:after {
  position: absolute;
  left: 0;
  right: 0;
  top: 100%;
  background: #3b93af;
  padding: 0 0 500px;
  margin: 0 0 -500px;
  z-index: 9999;
}

#header {
  width: 100%;
  position: fixed;
  z-index: 100;
  background: none;
  padding: 30px 0;
  -webkit-transition: all 0.2s linear;
  -o-transition: all 0.2s linear;
  transition: all 0.2s linear;
}
#header.sticky {
  background: #3b93af;
  padding: 13px 0;
}
.blog #header, .download #header {
  background: #3b93af;
}
.blog #header, .download #header {
  padding: 13px 0 0;
}
#header.menu-opened {
  background: #3b93af;
}
@media only screen and (max-width: 991px) {
  #header {
    padding: 13px 0 0;
  }
  #header.sticky {
    padding: 13px 0 0;
  }
  #header .navbar-header {
    padding: 0 0 13px;
  }
  .blog #header .navbar-header, .download #header .navbar-header {
    padding: 0;
  }
}

.navbar-default {
  border: 0;
}
.blog .navbar-default, .download .navbar-default {
  padding-bottom: 13px;
}
.blog .sticky .navbar-default, .download .sticky .navbar-default {
  padding-bottom: 13px;
}

.navbar-brand {
  padding: 0;
  width: 71px;
  height: 71px;
  position: relative;
  -webkit-transition: all 0.2s ease-in-out;
  -o-transition: all 0.2s ease-in-out;
  transition: all 0.2s ease-in-out;
}
.navbar-brand:before {
  content: '';
  position: relative;
  left: -3px;
  display: inline-block;
  vertical-align: middle;
  height: 100%;
  width: 0;
}
.navbar-brand img {
  display: inline-block;
  vertical-align: middle;
  line-height: 71px;
  width: 100%;
  max-width: none;
}
.navbar-brand:hover {
  opacity: 0.7;
}
.individual .navbar-brand {
  float: none;
  display: inline-block;
  vertical-align: top;
  margin-bottom: 30px;
}
@media only screen and (max-width: 767px) {
  .navbar-brand {
    width: 55px;
    height: 55px;
    margin: -5px 0 -5px 25px;
  }
}

.sticky .navbar-brand {
  width: 45px;
  height: 45px;
}
.sticky .navbar-brand:hover {
  opacity: 1;
}

.blog .navbar-brand,
.download .navbar-brand {
  width: 45px;
  height: 45px;
}

.navbar-default .navbar-toggle {
  padding: 0;
  border: 0;
  border-radius: 0;
  margin: 12px 25px 0 0;
  outline: none;
}
.navbar-default .navbar-toggle .icon-bar {
  border-radius: 0;
  width: 27px;
  margin: 0 0 8px;
}
.navbar-default .navbar-toggle.collapsed {
  background: none;
}
.navbar-default .navbar-toggle.collapsed .icon-bar {
  background: white;
}

.menu-active .collapsing {
  right: 0;
}

.navbar-collapse {
  -webkit-box-shadow: none;
  box-shadow: none;
}
.navbar-collapse.in {
  right: 0;
}
@media only screen and (max-width: 767px) {
  .navbar-collapse {
    background: #3b93af;
    position: fixed;
    top: -60px;
    padding: 60px 0 0;
    left: auto;
    right: -240px;
    bottom: 0;
    z-index: 999;
    width: 240px;
    display: block;
    -webkit-transition: all 0.3s linear;
    -o-transition: all 0.3s linear;
    transition: all 0.3s linear;
    height: auto !important;
  }
}

.navbar-nav {
  margin: 25px 0 0;
  font: 300 15px/1 "Roboto Slab", Georgia, serif;
  text-transform: uppercase;
  -webkit-transition: margin 0.2s ease-in-out;
  -o-transition: margin 0.2s ease-in-out;
  transition: margin 0.2s ease-in-out;
}
@media only screen and (max-width: 767px) {
  .navbar-nav {
    float: none !important;
    margin: 10px 10px 30px !important;
    font-size: 22px;
    font-weight: 300;
  }
}
.navbar-nav > li {
  padding-left: 75px;
}
@media only screen and (max-width: 991px) {
  .navbar-nav > li {
    padding-left: 40px;
  }
}
@media only screen and (max-width: 767px) {
  .navbar-nav > li {
    padding-left: 0;
  }
}
.navbar-nav > li > a {
  line-height: 21px;
  padding-bottom: 12px;
  border-bottom: 2px solid transparent;
  -webkit-transition: border-bottom-color 0.1s linear;
  -o-transition: border-bottom-color 0.1s linear;
  transition: border-bottom-color 0.1s linear;
}
.navbar-nav > li > a:hover {
  border-bottom-color: white;
}
@media only screen and (max-width: 767px) {
  .navbar-nav > li > a {
    height: 61px;
    padding: 18px 0;
    border-bottom: 1px solid #519ab1;
  }
  .navbar-nav > li > a:hover {
    border-bottom-color: white;
  }
}
.navbar-nav > .active > a {
  border-bottom-color: white;
}

.sticky .navbar-nav {
  margin: 10px 0 0;
}

.blog .navbar-nav,
.download .navbar-nav {
  margin: 10px 0 0;
}

.top-section {
  position: fixed;
  width: 100%;
  left: 0;
  top: 0;
  background: #b3302c;
  height: 720px;
  z-index: 1;
}
@media only screen and (max-width: 767px) {
  .top-section {
    height: 350px;
    -webkit-transition: all 0.3s linear;
    -o-transition: all 0.3s linear;
    transition: all 0.3s linear;
  }
}
.individual .top-section {
  height: 500px;
}
.filter-page .top-section {
  height: 450px;
}
@media only screen and (max-width: 1200px) {
  .filter-page .top-section {
    height: 495px;
  }
}
@media only screen and (max-width: 991px) {
  .filter-page .top-section {
    height: 400px;
  }
}
.top-section.contact {
  height: 500px;
}
.top-section.contact + #main {
  margin-top: 500px;
}

.video-section {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1;
 /* background: url(../images/video-still.jpg) no-repeat 50% 50%;
  background-size: cover;*/
}
@media only screen and (max-width: 479px) {
  .video-section {
    /*background: url(../images/video-still-mobile.jpg) no-repeat 50% 50%;
    background-size: cover;*/
  }
}
.images .video-section {
  background: none;
}

.top-section .bg-holder:after {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  content: '';
  background: rgba(59, 147, 175, 0.7);
  z-index: 1;
}

.video-frame {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.images .video-frame {
  bottom: 0;
  right: 0;
}
.images .video-frame img {
  height: 100%;
  width: 100%;
}
@media only screen and (max-width: 479px) {
  .images .video-frame img {
    max-width: none;
    width: auto;
  }
}
.video-frame:after {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  content: '';
  background: rgba(59, 147, 175, 0.7);
  z-index: 1;
}
.video-frame .mejs-controls {
  display: none !important;
}
.video-frame .video-box {
  width: 100%;
  overflow: hidden;
  position: absolute;
  left: 0;
  top: 0;
}
.video-frame .mejs-container {
  width: 100% !important;
  height: 100% !important;
}
.video-frame video {
  display: block;
  height: 100%;
  width: 100%;
}
.video-frame .mejs-inner,
.video-frame .mejs-mediaelement {
  height: 100%;
}

.no-video .video-box {
  display: none;
}

.video-section .holder {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  width: 100%;
 /* padding: 171px 0 0;*/
  padding: 205px 0 0;
  z-index: 3;
  color: white;
  text-align: center;
}
@media only screen and (max-width: 767px) {
  .video-section .holder {
    padding: 100px 25px 0;
  }
}
@media only screen and (max-width: 479px) {
  .video-section .holder {
    padding: 100px 25px 0;
    text-align: center;
  }
}
.video-section .holder .icon-contact {
  display: inline-block;
  vertical-align: top;
  font-size: 52px;
  padding: 0 5px 0 0;
}
.video-section .holder .link-call,
.video-section .holder .link-mail {
  color: white;
  display: inline-block;
  vertical-align: top;
  text-align: left;
  font-size: 24px;
}
.video-section .holder .link-call span,
.video-section .holder .link-mail span {
  display: block;
  font-size: 14px;
  line-height: 14px;
  margin-bottom: -3px;
}
.video-section .holder .social-links {
  padding: 15px 0;
}
@media only screen and (max-width: 992px) {
  .video-section .holder .col-xs-6 + h2 {
    display: none;
  }
  .video-section .holder .social-links {
    display: none;
  }
  .video-section .holder .social-links + h2 {
    display: none;
  }
  .video-section .holder .icon-contact {
    font-size: 46px;
  }
}
@media only screen and (max-width: 767px) {
  .video-section .holder .icon-contact {
    font-size: 40px;
    display: block;
    padding: 0 0 8px;
  }
  .video-section .holder .link-call, .video-section .holder .link-mail {
    font-size: 21px;
    text-align: center;
  }
}
@media only screen and (max-width: 540px) {
  .video-section .holder .icon-contact {
    display: none;
  }
  .video-section .holder .link-call, .video-section .holder .link-mail {
    font-size: 14px;
    text-align: center;
    word-break: break-all;
    line-height: 16px;
  }
  .video-section .holder .link-call span, .video-section .holder .link-mail span {
    padding-bottom: 4px;
  }
}
.video-section .holder h1 {
  font: 800 80px/80px "Lato", "Helvetica Neue", Helvetica, Arial, sans-serif;
  text-transform: uppercase;
  color: white;
  margin: 0 0 38px;
}
@media only screen and (max-width: 767px) {
  .video-section .holder h1 {
    font-size: 42px;
    line-height: 45px;
    margin: 0;
  }
}
@media only screen and (max-width: 479px) {
  .video-section .holder h1 {
    margin: 0 0 18px;
  }
  .video-section .holder h1 br {
    display: none;
